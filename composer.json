{
    "name": "rector/website",
    "description": "Website of GetRector.org",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "doctrine/annotations": "^1.8",
        "doctrine/cache": "^1.9",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "gedmo/doctrine-extensions": "^2.4",
        "psr/simple-cache": "^1.0",
        "ramsey/uuid": "^3.8",
        "ramsey/uuid-doctrine": "^1.5",
        "rector/rector": "^0.6.11",
        "sentry/sentry-symfony": "^3.3",
        "symfony/asset": "*",
        "symfony/cache": "*",
        "symfony/console": "*",
        "symfony/debug-bundle": "*",
        "symfony/dependency-injection": "*",
        "symfony/dotenv": "*",
        "symfony/event-dispatcher": "*",
        "symfony/flex": "^1.4",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-kernel": "*",
        "symfony/intl": "*",
        "symfony/mime": "*",
        "symfony/orm-pack": "^1.0.7",
        "symfony/process": "*",
        "symfony/security-bundle": "*",
        "symfony/stopwatch": "*",
        "symfony/templating": "*",
        "symfony/translation": "*",
        "symfony/twig-bridge": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-profiler-bundle": "*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "*",
        "symplify/auto-bind-parameter": "^7.2",
        "symplify/autodiscovery": "^7.2",
        "symplify/autowire-array-parameter": "^7.2",
        "symplify/flex-loader": "^7.2",
        "symplify/package-builder": "^7.2",
        "symplify/statie": "^7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.4",
        "phpstan/phpstan-doctrine": "^0.12",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "*",
        "symfony/browser-kit": "*",
        "symplify/coding-standard": "^7.2",
        "symplify/easy-coding-standard": "^7.2",
        "symplify/phpstan-extensions": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Rector\\Website\\": "src",
            "Rector\\Website\\Blog\\": "packages/Blog/src"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "check-cs": "vendor/bin/ecs check src --ansi",
        "fix-cs": "vendor/bin/ecs check src --fix --ansi",
        "doctrine": "bin/console doctrine:schema:update --dump-sql --force --ansi",
        "phpstan": "php -d memory_limit=1G vendor/bin/phpstan analyse src --error-format symplify --ansi",
        "rector": "vendor/bin/rector process src --config rector-ci.yaml --ansi"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.0.*"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
